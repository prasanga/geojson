{
    "name": "prasanga/geojson",
    "description": "A library for handling GeoJSON data",
    "require": {
        "php": "^7.2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "GeoJson\\": "src/"
        }
    },
    "scripts": {
        "unit": "phpunit",
        "cs": "phpcs",
        "build": [
            "@unit",
            "@cs"
        ]
    }
}
